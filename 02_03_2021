#include <iosteam> // allows you to print to screen
C++ decrementing while loop:

using namespace std;

int main(){
    int xx = 10;
    int Dec = 2;
    while (xx > 0){
        cout << xx << " , ";
        xx = xx - Dec;
    }
    return 0;
}

